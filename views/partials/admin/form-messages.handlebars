{{#if success_message}}
<div class="alert alert-success">
    {{success_message}}
</div>
{{/if}} {{#if error_message}}
<div class="alert alert-danger">
    {{error_message}}
</div>
{{/if}} {{#if error}}
<div class="alert alert-danger">
    {{error}}
</div>
{{/if}} {{#if info_message}}
<div class="alert alert-info">
    {{info_message}}
</div>
{{/if}}